<h1> New User </h1>

<form action="<%= users_url %>" method="post">
  <label for="username">Username: </label>
  <input type="text" id="username" name="user[username]" value="<%= @user.username %>">
  <br>
  <label for="password">Password: </label>
  <input type="text" id="password" name="user[password]" value="<%= @user.password %>">
  <br>
  <input type="submit" value="Sign up!"></label>
</form>